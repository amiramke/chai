<%= form_for(@visit) do |f| %>

  <%= f.label 'wifi:' %>
  <%=f.select :wifi, options_for_select((1..10))%>
  <%= f.label 'power sockets:'%>
  <%=f.select :power, options_for_select((1..10))%>
  <%= f.label 'atmosphere:'%>
  <%=f.select :atmosphere, options_for_select((1..10))%>
  <%= f.label 'shops:' %>
  <%= f.select :shop_id, options_from_collection_for_select(Shop.all, :id, :name) %>
  <%= f.submit "Rate your visit", :class => "btn btn-medium btn-primary" %>

<% end %>